{
  "name": "WebProfiler",
  "version": "1.1",
  "minimum_chrome_version": "10.0",
  "description": "Extends the Developer Tools, adding measuring tools for dependency graph.",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": { "scripts": ["js/background.js","js/jquery.min.js","js/plotly-latest.min.js"] },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
